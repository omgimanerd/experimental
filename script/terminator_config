  close_window = <Ctrl>Q
  go_down = <Ctrl>K
  go_left = <Ctrl>J
  go_right = <Ctrl>L
  go_up = <Ctrl>I
  new_tab = <Ctrl>T
  next_tab = <Ctrl>O
  prev_tab = <Ctrl>U
  new_terminator = <Ctrl>N
  search = <Ctrl>F
  split_horiz = <Ctrl>E
  split_vert = <Ctrl>S
  zoom_in = <Ctrl>equal
  zoom_out = <Ctrl>minus
