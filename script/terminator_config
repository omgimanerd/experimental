  focus = system
  suppress_multiple_term_dialog = True
  title_font = Sans 15
[keybindings]
  close_term = <Ctrl>W
  close_window = <Ctrl>Q
  go_down = <Ctrl>K
  go_left = <Ctrl>J
  go_right = <Ctrl>L
  go_up = <Ctrl>I
  new_tab = <Ctrl>T
  new_terminator = <Ctrl>N
  next_tab = <Ctrl>O
  prev_tab = <Ctrl>U
  reset_clear = <Ctrl><Shift>L
  search = <Ctrl>F
  split_horiz = <Ctrl>E
  split_vert = <Ctrl>S
  zoom_in = <Ctrl>equal
  zoom_out = <Ctrl>minus
